{
  "name": "Gitindex",
  "description": "",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "documents/hello.html",
    "default_icon": "logo.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*"
      ],
      "matches": [
        "https://github.com/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/search*"
      ],
      "css": [
        "styles/popup.css"
      ],
      "js": [
        "compiled/search.js"
      ]
    },
    {
      "matches": [
        "https://github.com/*/*"
      ],
      "js": [
        "compiled/repo.js"
      ]
    }
  ]
}